<% if @users.count === 0 %>
  <div class="no-chats-div">
    <p class="no-chats-p">No other users yet</p>
  </div>
<% else %>
  <div class="user-index-div">
    <div class="user-options-div">
      <% @users.each do |user| %>
        <% next if current_user === user %>

        <%= render "global_partials/user_request_options", locals: {user: user} %>
      <% end %>
    </div>
  </div>
<% end %>